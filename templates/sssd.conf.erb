[sssd]
config_file_version = 2
domains = ad.lib.cam.ac.uk
services = nss, pam

[domain/ad.lib.cam.ac.uk]
id_provider = ad
auth_provider = ad
access_provider = ad

ldap_id_mapping = True
ldap_schema = ad
ldap_idmap_range_min = 10000
ldap_idmap_range_max = 50000
ldap_idmap_range_size = 40000
ldap_idmap_default_domain = ad.lib.cam.ac.uk

default_shell = /bin/bash
fallback_homedir = /home/%u

